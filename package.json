        "js-yaml": "^3.14.0",
        "semver": "^7.3.2",
        "yargs": "^16.0.3"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "eslint": "^7.2.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.4",
        "gitlab-ci-lint": "^1.0.2",
        "klaw": "^3.0.0",
        "mocha": "^8.1.3",
        "nyc": "^15.1.0",
        "prettier": "^2.1.2"
