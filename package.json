{
    "name": "@hauslo/pipeline",
    "version": "0.1.3",
    "description": "Heavily opinionated automated test, build and deploy pipelines. Makes prod-faring development a joy.",
    "bin": "./cli.js",
    "scripts": {
        "test": "npx mocha test",
        "coverage": "npx nyc npm test",
        "prettier": "npx prettier --ignore-path .gitignore --check .",
        "prettier-apply": "npx prettier --ignore-path .gitignore --write .",
        "lint": "npx eslint .",
        "lint-apply": "npx eslint --fix ."
    },
    "engines": {
        "node": ">=12.18.4"
    },
    "os": [
